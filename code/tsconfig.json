{
  "compilerOptions": {
    "module": "esnext",
    "removeComments": true,
    "target": "esnext",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,

    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "dist"
  }
}